    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;500;600&family=Ubuntu:wght@500&display=swap"
        rel="stylesheet" />
    
    <link rel="stylesheet" href="coursereg.css">
    <title>Admin</title>
</head>
<body>
    <div class="container1">
        <!-- navigation bar starts -->
        <nav>
            <ul>
                <li>
                    <a href="#" class="logo">
                        <span class="nav-item">Welcome<br><%=massage%></span>
                    </a>
                </li>
                <li>
                    <a class="mar hov" href="/adminpage_home">
                        <i class="fas fa-home"></i>
                        <span class="nav-item">Home</span>
                    </a>
                </li>
                <li >
                    <a class="hov" href="/adminpage_course">
                        <i class="fas fa-book"></i>
                        <span class="nav-item">Courses</span>
                    </a>
                </li>
                <li>
                    <a class="hov" href="/adminpage_addcourse">
                        <i class="fas fa-file-circle-plus"></i>
                        <span class="nav-item">Add Course</span>
                    </a>
                </li>
                <li>
                    <a class="hov" href="adminpage_searchcourse">
                        <i class="fas fa-copy"></i>
