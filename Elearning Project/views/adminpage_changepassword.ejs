        <!-- navigation bar ends -->
        <div class="add">
           <h1>Change Password</h1>
           <form action="/pass" method="post" enctype="multipart/form-data">
                <!-- <span>Course Id</span> -->
                <h3><%=result%></h3>
                <input type="password" name="newpassword" placeholder="Enter New Password" class="in" id="" required><br>
                <input type="password" name="confirmnewpassword" placeholder="Confirm New Password" class="in" id="" required><br>
               
                <button type="submit" class="btn">Change</button>
           </form>
        </div>

        <!-- body section ends -->
       
   







